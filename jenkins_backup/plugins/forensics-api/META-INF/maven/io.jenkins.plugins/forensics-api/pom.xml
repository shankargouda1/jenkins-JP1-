    </dependency>
    <dependency>
      <groupId>org.jenkins-ci.plugins.workflow</groupId>
      <artifactId>workflow-cps</artifactId>
      <version>3853.vb_a_490d892963</version>
      <scope>compile</scope>
    </dependency>
